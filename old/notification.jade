  body
    div
      // BEGIN THEME SETTING
      #theme-setting
        .content-theme-setting
          select#list-style.form-control
            option(value='style1') Flat Squared style
            option(value='style2') Flat Rounded style
            option(value='style3', selected='selected') Flat Border style
      // END THEME SETTING
      // BEGIN TOPBAR
      #header-topbar-option-demo.page-header-topbar
        nav#topbar.navbar.navbar-default.navbar-static-top(role='navigation', style='margin-bottom: 0;', data-step='3')
          .navbar-header
            button.navbar-toggle(type='button', data-toggle='collapse', data-target='.sidebar-collapse')
              span.sr-only Toggle navigation
              span.icon-bar
              span.icon-bar
              span.icon-bar
            a#logo.navbar-brand(href='index.html')
              span.fa.fa-rocket
              span.logo-text Plant Buddy
              span.logo-text-icon(style='display: none') Âµ
      // END TOPBAR
      #wrapper
        // BEGIN SIDEBAR MENU
        nav#sidebar.navbar-default.navbar-static-side(role='navigation', data-step='2', data-intro='Template has <b>many navigation styles</b>', data-position='right')
          .sidebar-collapse.menu-scroll.collapse
            ul#side-menu.nav
              .clearfix
              li
                a(href='status.jade')
                  i.fa.fa-align-left.fa-fw
                    .icon-bg.bg-orange
                  span.menu-title Plant Status
              li
                a(href='myplant.jade')
                  i.fa.fa-leaf.fa-fw
                    .icon-bg.bg-orange
                  span.menu-title My Plant
              li.active
                a(href='notification.jade')
                  i.fa.fa-bell.fa-fw
                    .icon-bg.bg-pink
                  span.menu-title Notification
              li
                a(href='#')
                  i.fa.fa-sign-out.fa-fw
                    .icon-bg.bg-green
                  span.menu-title Log Out
        #page-wrapper
          // BEGIN CONTENT
          .page-content
            #tab-general
              .row.mbl
                .col-lg-12
                  .col-md-12
                    #area-chart-spline(style='width: 100%; height: 300px; display: none;')
                .col-lg-12
                  .row
                    h2 Notification
                    .col-lg-6
                      .panel.panel-green
                        table.table.table-hover-color
                          tbody
                            tr
                              td
                                i.fa.fa-tint(style='color:#2AEB9B;')
                                | &nbsp&nbspWatered
                              td 9 mins ago
                            tr
                              td
                                i.fa.fa-fire(style='color:#E75B5E;')
                                | &nbsp&nbspHigh Temparature
                              td 10 mins ago
                            tr
                              td
                                i.fa.fa-heart(style='color:#2AEB9B;')
                                | &nbsp&nbspApply Fertilizer
                              td 60 mins ago
                            tr
                              td
                                i.fa.fa-fire(style='color:#2AEB9B;')
                                | &nbsp&nbspLow Temparature
                              td 1 day ago
            // END CONTENT
          // END PAGE WRAPPER
